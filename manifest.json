{
    "browser_action": {
        "default_icon": "icons/icon.png",
        "default_popup": "popup.html",
        "default_title": "Grammars"
    },
    "description": "Send text to your phone to read",
    "homepage_url": "https://github.com/ggreer/history-stats",
    "icons": {
        "128": "icons/icon128.png",
        "48": "icons/icon48.png"
    },
    "manifest_version": 2,
    "name": "Grammars",
    "permissions": [
        "<all_urls>",
        "chrome://favicon/",
        "history",
        "tabs",
        "contextMenus"
    ],
      "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.3.2-vsdoc2.js","hello.js"]
    }
  ],
    
    "version": "0.1"
}
